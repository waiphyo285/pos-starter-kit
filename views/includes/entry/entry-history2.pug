mixin entryHistory(options)
  include ../other/breadcrumb
    block items
      - var breadcrumbList = page[options.breadcrumbKey].breadcrumb;
      if breadcrumbList.length > 0
        each breadcrumbObj, index in breadcrumbList
          - var cl = breadcrumbList.length == index ? 'font-weight-bold' : ''
          li.breadcrumb-item.d-none.d-md-inline
            a(href=breadcrumbObj.url style='color: var(--primary)' class=cl) 
              | !{breadcrumbObj.text}k