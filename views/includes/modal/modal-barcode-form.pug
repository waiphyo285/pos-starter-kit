div.modal.fade#dialogBarcodeForm(tabindex='-1', role='dialog', aria-labelledby='dialogBarcodeForm')
  div.modal-dialog.modal-dialog-centered.modal-md(role='document')
    div.modal-content
      div.modal-header
        h5.modal-title !{content.modal.print_barcode}
        button.close(type='button', data-dismiss='modal', aria-label='Close')
          span(aria-hidden='true') &times;
      
      div.modal-body
        div#barcodeList.barcodeList.align-center
          | No barcode found!

      div.modal-footer.justify-content-md-center
        button.btn.material-button.material-secondary#dialogCancel(type='button', data-dismiss='modal')
          |  !{content.common.btn_close}

        button.btn.material-button.material-primary.ml-1#dialogBarcode(type='button' role="button" onclick="return printBarcode()" data-dismiss='modal')
          i.bi.bi-printer
          |  !{content.common.btn_print} 

  script.
    $(document).ready(function() {})

    function printBarcode() {
      printJS({
        type: 'html',
        printable: 'barcodeList',
        //- css: '/stylesheet/view-invoice.css'
      })
    }
